{
  "extends": [
    "tslint:recommended",
    "tslint-config-standard"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": false,
    "max-line-length": [true, 300],
    "no-conditional-assignment": false,
    "no-console": [true, "log"],
    "object-literal-sort-keys": false,
    "triple-equals": [true, "allow-null-check"]
  }
}
